{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/dex-proxy", "dest": "/api/dex-proxy.js" },
    { "src": "/api/pump-proxy", "dest": "/api/pump-proxy.js" },
    { "src": "/api/solana", "dest": "/api/solana.js" },
    { "src": "/api/token", "dest": "/api/token.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
